kind: Role
apiVersion: rbac.authorization.k8s.io/v1beta1
metadata:
  namespace: dev
  name: username-role-manager
rules:
- apiGroups: [""]
  resources: ["deployments", "pods", "pods/log", "services", "services/proxy"]
  verbs: ["get", "list", "describe"] 
- apiGroups: [""]
  resources: ["configmaps"]
  resourceNames: ["global-config", "canary-global-config", "project-config", "canary-project-config"]
  verbs: ["get", "list"]
